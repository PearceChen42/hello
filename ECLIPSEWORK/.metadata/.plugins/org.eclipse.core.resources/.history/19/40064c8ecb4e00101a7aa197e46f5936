
public class Car {
	
	private String make;
	private String model;
	private int year;
	
	Car(String make, String model, int year ) {
		setMake(make);
		setMode(model);
		setYear(year);
		
	}
	
	public String getMake() {
		return make;
	}
	
	public String getModel() {
		return model;
	}
	
	public int getYear() {
		return year;
		
	}
	
	public void setMake(String make) {
		this.make = make;
		
	}
	
	public void setMode(String mode) {
		this.model=mode;
		
	}
	
	public void setYear(int year) {
		this.year = year;
		
	}
	
	public void copy(Car x) {
		this.setMake(x.make);
		this.setMode(x.model);
		this.setYear(x.year);
	}
	

}
